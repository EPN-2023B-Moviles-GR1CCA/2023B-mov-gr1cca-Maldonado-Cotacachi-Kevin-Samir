<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="579dp"
        android:layout_height="339dp"
        android:text="Nombre"
        tools:layout_editor_absoluteX="555dp"
        tools:layout_editor_absoluteY="1226dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="739dp"
        android:layout_height="323dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="2295dp"
        tools:layout_editor_absoluteY="1226dp" />

    <TextView
        android:id="@+id/tv_nombre"
        android:layout_width="129dp"
        android:layout_height="36dp"
        android:layout_marginEnd="84dp"
        android:text="NOMBRE"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_descripcion"
        app:layout_constraintEnd_toStartOf="@+id/tv_descripcion"
        app:layout_constraintTop_toTopOf="@+id/tv_descripcion"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tv_descripcion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripcion"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.495" />

    <TextView
        android:id="@+id/tv_likes"
        android:layout_width="91dp"
        android:layout_height="33dp"
        android:layout_marginStart="100dp"
        android:text="Likes"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_descripcion"
        app:layout_constraintStart_toEndOf="@+id/tv_descripcion"
        app:layout_constraintTop_toTopOf="@+id/tv_descripcion"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn_dar_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="DAR LIKE"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_descripcion"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/tv_descripcion"
        app:layout_constraintTop_toBottomOf="@+id/tv_descripcion" />
</androidx.constraintlayout.widget.ConstraintLayout>